; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org
; top-most EditorConfig file
root = true

; Unix-style newlines
[*]
end_of_line = LF
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

[*.php]
indent_style = space
indent_size = 4
